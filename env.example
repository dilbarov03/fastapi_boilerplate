PROJECT_NAME="FastAPI Boilerplate"

# Database
# Sync URL for Alembic/Admin
SQLALCHEMY_DATABASE_URI="postgresql://postgres:password123@localhost:5432/app_db"
# Async URL for FastAPI
ASYNC_SQLALCHEMY_DATABASE_URI="postgresql+asyncpg://postgres:password123@localhost:5432/app_db"

# Security (generate with: openssl rand -hex 32)
SECRET_KEY="your-secret-key-change-this-in-production"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Admin
FIRST_SUPERUSER="admin"
FIRST_SUPERUSER_PASSWORD="admin123"

# CORS
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]